<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Clientes</title>
</head>
<body>
  <h1>Clientes</h1>
  <table border="1">
    <thead>
      <tr>
        <th>ID</th>
        <th>Nombre</th>
        <th>Apellido</th>
        <th>Email</th>
        <th>Tel√©fono</th>
      </tr>
    </thead>
    <tbody>
      <% clients.forEach(client => { %>
        <tr>
          <td><%= client.id_client %></td>
          <td><%= client.name %></td>
          <td><%= client.surname %></td>
          <td><%= client.email %></td>
          <td><%= client.phone %></td>
        </tr>
      <% }); %>
    </tbody>
  </table>
</body>
</html>
